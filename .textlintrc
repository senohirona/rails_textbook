{
    "rules": {
        "preset-ja-technical-writing": {
            "max-ten": {
                "max": 5
            },
            "max-kanji-continuous-len": {
                max: 6,
                allow: ["郵便番号入力欄","一橋大学社会学部"]
            },
            "no-doubled-joshi": {
                "min_interval" : 1,
                "strict": false,
                "allow": ["に","で","か","も","や", "は","が","たり","ば"]
            },
            "sentence-length": {
                "max": 200
            },
            "ja-no-weak-phrase": false,
            "no-exclamation-question-mark": false
        }
    }
}
